MODULE main
VAR
	state : {0,1,2,3,9,10,11,12,13};
ASSIGN
	init(state) := 2;
	next(state) := case
		state = 0: {0,9};
		state = 1: {1,10};
		state = 2: {2,11};
		state = 3: {3,12};
		state = 9: {0,9,10,11};
		state = 10: {1,10,11};
		state = 11: {2,11};
		state = 12: {3,12};
		state = 13: 13;
	esac;
